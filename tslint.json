{
  "extends": [
    "tslint-clean-code"
  ],
  "rules": {
    "newspaper-order": true
  },
  "defaultSeverity": "warning",
   "linterOptions": {
     "exclude": [
       "config/**/*.js",
       "node_modules/*",
       "node_modules/@types/*"
     ]
  },
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react"
  },
}